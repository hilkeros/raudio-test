Load mix:
<%= form_tag mix_path(1), remote: true, method: :get do %>
	<%= select_tag :mix_id, options_for_select(Mix.where(file: file).map{|m| [m.name, m.id]}) %>
	<%= submit_tag 'Load' %>
<% end %>