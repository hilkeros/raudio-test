Current mix: <strong><%= name %></strong>
<% if mix.present? %>
	<%= form_for mix, url: mix_path(mix), html: { method: :put, remote: true, class: 'getKnobs' } do |f| %>
		<%= f.submit 'Update' %>
	<% end %>
<% end %>
