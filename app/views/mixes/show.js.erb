$('#mix-loader').html("<%= j render 'mixes/mix_loader', mix: @mix, file: @mix.file, name: @mix.name %>");
var knobs = <%= @mix.knobs.to_json.html_safe %>;
$('.volume').each(function(index) {
			window[$(this).attr('id')].value = knobs['volumes'][index];
});
$('.send').each(function(index) {
			window[$(this).attr('id')].value = knobs['sends'][index];
});
get_knobs();